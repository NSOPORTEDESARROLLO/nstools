#!/bin/bash

NSTOOLS_PATH="/opt/nstools"
	. "$NSTOOLS_PATH/lib/astman_cli"


trap "exec $NSTOOLS_PATH/bin/gui_astman_cli" SIGINT


echo "

"
echo "Digite el comando CLI o digite r para regresar"
read CMD


if [ "$CMD" = "r" ];then
	exec $NSTOOLS_PATH/bin/cat_voip
fi


AstmanCli "$CMD"

exec $NSTOOLS_PATH/bin/gui_astman_cli






